{
    "name": "chat-frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:mock": "vite --mode mock",
        "build": "vue-tsc -b && vite build",
        "preview": "vite preview",
        "prepare": "husky"
    },
    "lint-staged": {
        "*.js": [
            "prettier --write"
        ],
        "*.ts": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.vue": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.css": [
            "prettier --write"
        ],
        "*.json": [
            "prettier --write"
        ],
        "*.md": [
            "prettier --write"
        ]
    },
    "dependencies": {
        "@types/webrtc": "^0.0.44",
        "axios": "^1.7.7",
        "eslint-plugin-vue": "^9.30.0",
        "normalize.css": "^8.0.1",
        "pinia": "^2.2.6",
        "vant": "^4.9.8",
        "vue": "^3.4.21",
        "vue-router": "4",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@types/dom-webcodecs": "^0.1.13",
        "@vitejs/plugin-vue": "^5.1.4",
        "eslint": "^9.14.0",
        "globals": "^15.11.0",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "mockjs": "^1.1.0",
        "postcss": "^8.4.47",
        "postcss-px-to-viewport": "^1.1.1",
        "prettier": "^3.3.3",
        "sass": "^1.80.5",
        "typescript": "~5.2.2",
        "typescript-eslint": "^8.12.2",
        "vite": "^5.4.10",
        "vite-plugin-mock": "^3.0.2",
        "vue-tsc": "^2.0.0"
    }
}
